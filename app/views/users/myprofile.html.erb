<div class"row">
  <div class="col-lg-3">
  <% if @user.image_url %>
    <%= image_tag(@user.image_url, :alt => "profile image") %><br>
  <% else %>
    <%= image_tag("/images/blank_profile.png", :alt => "login with Facebook to get an image") %><br>
  <% end %>
  </div>
  <div class="col-lg-3">
    <br>
    <strong>First Name:</strong> <%= @user.first_name %><br>
    <strong>Last Name: </strong><%= @user.last_name %><br>
    <strong>Email: </strong><%= @user.email %><br><br>
    <strong>Address: </strong><%= @user.address %><br>
    <strong>Born on: </strong><%= @user.born_on %><br>
    <strong>Born at: </strong><%= @user.born_at %><br>
    <strong>Telephone: </strong><%= @user.telephone %><br>
    <strong>Website: </strong><%= @user.website %><br><br>
    <strong>Social Number: </strong><%= @user.social_number %><br>
    <strong>ID Number: </strong><%= @user.id_number %><br>
    <strong>License Number: </strong><%= @user.license_number %><br>
    <strong>Insurance Company: </strong><%= @user.insurance_company %><br>
    <strong>Car Plate: </strong><%= @user.car_plate %><br><br>
    <strong>Weight: </strong><%= @user.weight %><br>
    <strong>Height: </strong><%= @user.height %><br>
    <strong>Blood group: </strong><%= @user.blood_group %><br>
    <br>
    <% if !@user.provider %>
    <%= link_to 'Edit LogIn Info', edit_user_registration_path, :class => 'navbar-link' %> |
    <% end %>
    <%= link_to 'Edit Profile', edit_user_path(@user), :class => 'navbar-link' %> |
    <%= link_to "Home", root_path, :class => 'navbar-link' %>
  </div>
  <div class="col-lg-3">
	<br>
	<%= button_to "Blocked Users List", blockedusers_show_path, method: :get , :class => 'btn btn-danger'%>
  </div>
</div>
