<% @users.each do |user| %>
  <% if user.image_url %>
    <%= image_tag(user.image_url, :alt => "profile image") %><br>
  <% else %>
    <%= image_tag("/images/blank_profile.png", :alt => "login with Facebook to get an image") %><br>
  <% end %>
  <%= user.first_name %>
  <%= user.last_name %>
  <%= link_to 'Show', users_show_path(:id => user.id) %>
  <br>
<% end %>
<%= link_to 'Back', session.delete(:return_to) %>
